[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Iosevka Term 14 
border_width = 0
scrollback_lines = 10000
search_wrap = true
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = system

# "block", "underline" or "ibeam"
cursor_shape = block

# $BROWSER is used by default if set, with xdg-open as a fallback
#browser = xdg-open

# set size hints for the window
#size_hints = false

# Hide links that are no longer valid in url select overlay mode
filter_unmatched_urls = true

# emit escape sequences for extra modified keys
#modify_other_keys = false

# Dracula, from the Xresources theme linked on its main repo
# https://github.com/dracula/xresources/blob/fa6a33dfc6f270b643fee32713a90225f9b0d4fc/Xresources
[colors]

# special
foreground = #FFFFFF
foreground_bold = #FFFFFF
cursor = 
background = #000000

# black
color0 = #FFFF00
color8 = #FFFF08

# red
color1 = #FFFF01
color9 = #FFFF09

# green
color2 = #FFFF02
color10 = #FFFF10

# yellow
color3 = #FFFF03
color11 = #FFFF11

# blue
color4 = #FFFF04
color12 = #FFFF12

# magenta
color5 = #FFFF05
color13 = #FFFF13

# cyan
color6 = #FFFF06
color14 = #FFFF14

# white
color7 = #FFFF07
color15 = #FFFF15
