{
  "extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
  "plugins": ["prettier", "node"],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": 0
  },
  "overrides": [{
    "files": "**/*.test.js",
    "rules": {
        "node/no-unpublished-require": 0,
        "node/no-missing-require": 0
    }
  }]
}
